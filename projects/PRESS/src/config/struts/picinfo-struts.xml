<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
   <package name="picinfo" namespace="/picinfo" extends="default">
		<action name="picinfo_*" class="picinfoAction" method="{1}">
			<result name="to_view">/WEB-INF/jsp/picinfo/picinfo_view.jsp</result>
			<result name="to_update">/WEB-INF/jsp/picinfo/picinfo_update.jsp</result>
			<result name="redirect_index" type="redirect">/picinfo/picinfo_to_index.action</result>
		</action>
	</package>
	
</struts>

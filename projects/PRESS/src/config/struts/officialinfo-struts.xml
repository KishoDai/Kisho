<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
   <package name="officialinfo" namespace="/officialinfo" extends="default">
		<action name="officialinfo_*" class="officialinfoAction" method="{1}">
		    <result name="to_iframe">/WEB-INF/jsp/comm/iframe.jsp</result>
			<result name="to_index">/WEB-INF/jsp/officialinfo/officialinfo_update.jsp</result>
			<result name="redirect_index" type="redirect">/officialinfo/officialinfo_to_index.action</result>
		</action>
	</package>
	
</struts>
